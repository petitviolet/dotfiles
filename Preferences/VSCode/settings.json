{
  "editor.fontSize": 16,
  "vim.hlsearch": true,
  "vim.useCtrlKeys": true,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-…、。「」【】『』（）！？　てにをはがのともへでや",
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [{
      "before": [
        "<C-n>"
      ],
      "after": [
        "<C-o>",
        "j"
      ]
    },
    {
      "before": [
        "<C-p>"
      ],
      "after": [
        "<C-o>",
        "k"
      ]
    },
    {
      "before": [
        "<C-a>"
      ],
      "after": [
        "<C-o>",
        "I"
      ]
    },
    {
      "before": [
        "<C-e>"
      ],
      "after": [
        "<C-o>",
        "A"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [{
      "before": [
        "j"
      ],
      "after": [
        "g",
        "j"
      ]
    },
    {
      "before": [
        "k"
      ],
      "after": [
        "g",
        "k"
      ]
    },
    {
      "before":["<Esc>"],
      "commands": [
        ":nohl"
      ]
    }
  ],
  "zenMode.hideTabs": false,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "[markdown]": {
    "editor.tabSize": 4,
    "editor.quickSuggestions": true
  },
  "editor.insertSpaces": true,
  "markdown.preview.fontSize": 20,
  "markdownlint.run": "onSave",
  "markdown.extension.toc.githubCompatibility": true,
  "workbench.iconTheme": "vs-seti",
  "prettier.disableLanguages": [
    "markdown",
    "md"
  ],
  "json.format.enable": true,
  "markdown.extension.toc.levels": "1..3",
  "pasteImage.basePath": "${currentFileDir}/assets",
  "pasteImage.path": "${currentFileDir}/assets",
  "languageTool": {
    "enabled": true,
    "language": "en"
  },
  "explorer.confirmDelete": false,
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 100,
  "workbench.commandPalette.history": 9999,
  "rest-client.defaultHeaders": {
    "User-Agent": "vscode-restclient",
    "Accept-Encoding": "gzip"
  },
  "markdown.extension.list.indentationSize": "inherit",
  "vscode-markdown.config": {
    "lists": {
      "indent": 2
    }
  },
  "editor.detectIndentation": false,
  "[jsonc]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "explorer.confirmDragAndDrop": false,
  "editor.cursorBlinking": "smooth",
  // "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "telemetry.enableTelemetry": false,
  "workbench.editor.highlightModifiedTabs": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.editor.enablePreview": false,
  "ruby.intellisense": "rubyLocate",
  "ruby.useLanguageServer": true,
  "solargraph.definitions": true,
  "solargraph.diagnostics": true,
  "files.autoSave": "onWindowChange",
  "editor.multiCursorModifier": "alt",
  "editor.tabCompletion": "on",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.pythonPath": "/usr/local/bin/python3",
  // "lldb.libpython": "/usr/local/Cellar/python/3.7.7/Frameworks/Python.framework/Versions/3.7/lib/libpython3.7.dylib",
  "lldb.libpython": "/usr/local/Cellar/python@3.8/3.8.5/Frameworks/Python.framework/Versions/3.8/lib/libpython3.8.dylib",
  "rust-client.channel": "stable",
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "ruby.codeCompletion": "rcodetools",
  "ruby.lint": {
  },
  "solargraph.formatting": true,
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "redhat.telemetry.enabled": true,
  "editor.fontFamily": "Ricty, Menlo, Monaco, 'Courier New', monospace",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.onTaskErrors": "showErrors",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "workbench.startupEditor": "none",
  "explorer.autoReveal": false,
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.fileAnnotations.command": "blame",
  "gitlens.currentLine.enabled": false,
  "gitlens.mode.active": "zen",
  "cSpell.userWords": [
    "Behaviour",
    "chakra",
    "datasources",
    "dryrun",
    "firestore",
    "idempotency",
    "Mirrativ",
    "petitviolet",
    "ulid",
    "upsert"
  ],
  "jestrunner.jestCommand": "npx env-cmd -f env/.env.testLocal jest",
  "editor.formatOnSave": true,
  "[graphql]": {
    "editor.formatOnSave": false
  },
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 12,
  "workbench.editor.closeEmptyGroups": false,
  "omnisharp.useGlobalMono": "always",
  "omnisharp.useModernNet": false,
  "omnisharp.monoPath":"/Library/Frameworks/Mono.framework/Versions/Current"
}
